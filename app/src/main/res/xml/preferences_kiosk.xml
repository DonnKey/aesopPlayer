    <SwitchPreference
        app:key="kiosk_mode_preference"
        app:title="@string/pref_kiosk_mode_full_title"
        app:defaultValue="false"/>
    <SwitchPreference
        app:key="simple_kiosk_mode_preference"
        app:title="@string/pref_kiosk_mode_simple_title"
